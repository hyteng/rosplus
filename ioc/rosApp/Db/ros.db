grecord(stringin,"$(P)ROS:$(Q):CMDBACK") {
        field(DTYP, "stream")
        field(INP, "@ros.proto getInput $(PORT) $(ADDR)")
}
grecord(stringout,"$(P)ROS:$(Q):CTRL") {
        field(DTYP, "stream")
        field(OUT, "@ros.proto setOutput($(P)ROS:$(Q):CTRL) $(PORT) $(ADDR)")
        field (FLNK, "$(P)ROS:$(Q):CMDBACK")
}
